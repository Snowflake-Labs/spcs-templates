
USE ROLE ACCOUNTADMIN;
USE {{DATABASE}}.{{SCHEMA}};

CREATE OR REPLACE NETWORK RULE WANDB_RULE
  MODE = EGRESS
  TYPE = HOST_PORT
  VALUE_LIST = ('wandb.ai:443');


CREATE OR REPLACE EXTERNAL ACCESS INTEGRATION WANDB_EAI
  ALLOWED_NETWORK_RULES = (WANDB_RULE)
  ENABLED = true;

GRANT USAGE ON INTEGRATION WANDB_EAI TO ROLE {{ROLE}};
USE ROLE {{ROLE}};
